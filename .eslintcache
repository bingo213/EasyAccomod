Loading(false);\r\n    };\r\n    fetchRentalUnitData();\r\n  }, []);\r\n\r\n  const indexOfLastRental = currentPage * rentalUnitPerPage;\r\n  const indexOfFirstRental = indexOfLastRental - rentalUnitPerPage;\r\n  const currentRentals = rentalUnit.slice(\r\n    indexOfFirstRental,\r\n    indexOfLastRental\r\n  );\r\n\r\n  const paginate = pageNumber => setCurrentPage(pageNumber);\r\n  return (\r\n    <div className=\"Home\" style={{ overflowX: 'hidden' }}>\r\n      <NavBar isLogin={isLogin} username={username} role={role} avatar={avatar}/>\r\n      <div className=\"homeImage\"></div>\r\n      <SearchBar />\r\n      <ListRentalUnit currentRentals={currentRentals} loading={loading} isLogin={isLogin} />\r\n      <Pagination\r\n        rentalUnitPerPage={rentalUnitPerPage}\r\n        totalRentalUnit={rentalUnit.length}\r\n        paginate={paginate}\r\n      />\r\n      <Footer />\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Home;\r\n","D:\\Learning\\Năm 3\\Kì 1\\Phát triển ứng dụng Web\\btl\\EasyAccomod\\src\\component\\detail_rental_unit\\DetailRentalUnit.js",["194","195","196","197","198","199","200","201","202","203","204"],"D:\\Learning\\Năm 3\\Kì 1\\Phát triển ứng dụng Web\\btl\\EasyAccomod\\src\\component\\login_register\\RenterRegister.js",["205"],"import React from 'react';\r\nimport Register from './Register';\r\nimport config from 'config'\r\n\r\nfunction RenterRegister() {\r\n  const fields = [\r\n    {\r\n      key: 1,\r\n      label: 'Họ tên',\r\n      type: 'text',\r\n      name: 'fullname',\r\n      registerObj: {\r\n        required: 'Bạn cần nhập họ tên',\r\n      },\r\n    },\r\n    {\r\n      key: 2,\r\n      label: 'Email',\r\n      type: 'email',\r\n      name: 'email',\r\n      registerObj: {\r\n        required: 'Bạn cần nhập email',\r\n        pattern: /^[^@ ]+@[^@ ]+\\.[^@ .]{2,}$/,\r\n      },\r\n    },\r\n    {\r\n      key: 3,\r\n      label: 'Tên tài khoản',\r\n      type: 'text',\r\n      name: 'username',\r\n      registerObj: {\r\n   